{
    "name": "@lwrjs/o11y",
    "license": "MIT",
    "publishConfig": {
        "access": "public"
    },
    "version": "0.7.4",
    "homepage": "https://developer.salesforce.com/docs/platform/lwr/overview",
    "repository": {
        "type": "git",
        "url": "https://github.com/salesforce/lwr.git",
        "directory": "packages/@lwrjs/o11y"
    },
    "bugs": {
        "url": "https://github.com/salesforce/lwr/issues"
    },
    "type": "module",
    "files": [
        "build/**/*.js",
        "build/**/*.d.ts"
    ],
    "scripts": {
        "build": "node ../../../bin/pack-lwc --dir modules build/modules && yarn build:bundle",
        "build:bundle": "rollup --config scripts/rollup.moduleBundle.config.cjs"
    },
    "dependencies": {
        "@lwrjs/shared-utils": "0.7.4",
        "o11y": "240.7.0"
    },
    "devDependencies": {
        "rollup-plugin-terser": "^7.0.2"
    },
    "lwc": {
        "modules": [
            {
                "dir": "build/modules"
            },
            {
                "npm": "o11y"
            }
        ],
        "expose": [
            "lwr/o11yService"
        ]
    },
    "engines": {
        "node": ">=14.15.4 <19"
    },
    "gitHead": "bdc524b077804f0b80da8f1880789cd8c17bb4f1"
}
